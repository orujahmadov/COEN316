# do file to test the regfile
add wave rt
add wave rs
add wave pc
add wave target_address
add wave branch_type
add wave pc_sel

# pc increment
force rt 00000000000000000000000000000000
force rs 00000000000000000000000000000000
force pc 00000000000000000000000000000001
force target_address 00000000000000001000100000
force branch_type 00
force pc_sel 00
run 5

# jump
force rt 00000000000000000000000000000000
force rs 00000000000000000000000000000000
force pc 00000000000000000000001000000000
force target_address 00000000000000000000100000
force branch_type 00
force pc_sel 01
run 5

# jump register
force rt 00000000000000000000000000000000
force rs 00000000001100000000000000000000
force pc 00000000000000000000001000000000
force target_address 00000000000000001000100000
